@article{heEvaluatingMisclassificationEffects2022,
  title = {Evaluating {{Misclassification Effects}} on {{Single Sequential Treatment}} in {{Sequential Multiple Assignment Randomized Trial}} ({{SMART}}) {{Designs}}},
